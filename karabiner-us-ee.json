{
   "title":"Estonian keyboard layout (üõöä) for US keyboards (as alt keys)",
   "rules":[
      {
         "description":"quote to ä/Ä",
         "manipulators":[
            {
               "type":"basic",
               "from":{
                  "modifiers":{
                     "mandatory":[
                        "left_alt"
                     ]
                  },
                  "key_code":"quote"
               },
               "to":[
                  {
                     "key_code":"u",
                     "modifiers":[
                        "left_option"
                     ]
                  },
                  {
                     "key_code":"a"
                  },
                  {
                     "key_code":"vk_none"
                  }
               ]
            },
            {
               "type":"basic",
               "from":{
                  "modifiers":{
                     "mandatory":[
                        "left_alt",
                        "left_shift"
                     ]
                  },
                  "key_code":"quote"
               },
               "to":[
                  {
                     "key_code":"u",
                     "modifiers":[
                        "left_option"
                     ]
                  },
                  {
                     "key_code":"a",
                     "modifiers":[
                        "left_shift"
                     ]
                  },
                  {
                     "key_code":"vk_none"
                  }
               ]
            }
         ]
      },
      {
         "description":"semicolon to ö/Ö",
         "manipulators":[
            {
               "type":"basic",
               "from":{
                  "modifiers":{
                     "mandatory":[
                        "left_alt"
                     ]
                  },
                  "key_code":"semicolon"
               },
               "to":[
                  {
                     "key_code":"u",
                     "modifiers":[
                        "left_option"
                     ]
                  },
                  {
                     "key_code":"o"
                  },
                  {
                     "key_code":"vk_none"
                  }
               ]
            },
            {
               "type":"basic",
               "from":{
                  "modifiers":{
                     "mandatory":[
                        "left_alt",
                        "left_shift"
                     ]
                  },
                  "key_code":"semicolon"
               },
               "to":[
                  {
                     "key_code":"u",
                     "modifiers":[
                        "left_option"
                     ]
                  },
                  {
                     "key_code":"o",
                     "modifiers":[
                        "left_shift"
                     ]
                  },
                  {
                     "key_code":"vk_none"
                  }
               ]
            }
         ]
      },
      {
         "description":"open_bracket to ü/Ü",
         "manipulators":[
            {
               "type":"basic",
               "from":{
                  "modifiers":{
                     "mandatory":[
                        "left_alt"
                     ]
                  },
                  "key_code":"open_bracket"
               },
               "to":[
                  {
                     "key_code":"u",
                     "modifiers":[
                        "left_option"
                     ]
                  },
                  {
                     "key_code":"u"
                  },
                  {
                     "key_code":"vk_none"
                  }
               ]
            },
            {
               "type":"basic",
               "from":{
                  "modifiers":{
                     "mandatory":[
                        "left_alt",
                        "left_shift"
                     ]
                  },
                  "key_code":"open_bracket"
               },
               "to":[
                  {
                     "key_code":"u",
                     "modifiers":[
                        "left_option"
                     ]
                  },
                  {
                     "key_code":"u",
                     "modifiers":[
                        "left_shift"
                     ]
                  },
                  {
                     "key_code":"vk_none"
                  }
               ]
            }
         ]
      },
      {
         "description":"close_bracket to õ/Õ",
         "manipulators":[
            {
               "type":"basic",
               "from":{
                  "modifiers":{
                     "mandatory":[
                        "left_alt"
                     ]
                  },
                  "key_code":"close_bracket"
               },
               "to":[
                  {
                     "key_code":"n",
                     "modifiers":[
                        "left_option"
                     ]
                  },
                  {
                     "key_code":"o"
                  },
                  {
                     "key_code":"vk_none"
                  }
               ]
            },
            {
               "type":"basic",
               "from":{
                  "modifiers":{
                     "mandatory":[
                        "left_alt",
                        "left_shift"
                     ]
                  },
                  "key_code":"close_bracket"
               },
               "to":[
                  {
                     "key_code":"n",
                     "modifiers":[
                        "left_option"
                     ]
                  },
                  {
                     "key_code":"o",
                     "modifiers":[
                        "left_shift"
                     ]
                  },
                  {
                     "key_code":"vk_none"
                  }
               ]
            }
         ]
      }
   ]
}
